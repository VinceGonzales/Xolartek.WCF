@using Xolartek.Entities;
@model List<Material>
@{
    ViewBag.Title = "Home Page";
}

<div class="container-fluid">
    <div class="row">
        <div class="col-sm-6">
            <h3>HOME PAGE</h3>
        </div>
        <div class="col-sm-6">
            @foreach(Material schema in Model)
            {
                <div>@schema.Description</div>
            }
        </div>
    </div>
</div>